<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="idv.people.login">

	<class name="LoginUser" table="TestLoginUser">
	
		<id name="uid" column="uid" type="java.lang.Integer">
			<generator class="increment" />
		</id>

		<property name="user" column="user" type="java.lang.String" not-null="true" />
		<property name="pwd" column="pwd" type="java.lang.String" not-null="true" />

	</class>
	
</hibernate-mapping>